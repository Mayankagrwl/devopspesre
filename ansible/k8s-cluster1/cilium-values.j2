kubeProxyReplacement: strict
k8sServiceHost: {{ ansible_host }}
externalIPs:
    enabled: true
k8sServicePort: 6443
bpf:
  hostLegacyRouting: false
  masquerade: true
prometheus:
  enabled: true
operator:
  prometheus:
    enabled: true
  metrics:
    enableOpenMetrics: true
